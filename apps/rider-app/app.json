{
  "expo": {
    "name": "rider-app",
    "slug": "rider-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hatod-logo.png",
    "scheme": "riderapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#C2185B",
        "foregroundImage": "./assets/images/hatod-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kridl.riderapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/hatod-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsDownloadToken": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/hatod-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#C2185B",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
